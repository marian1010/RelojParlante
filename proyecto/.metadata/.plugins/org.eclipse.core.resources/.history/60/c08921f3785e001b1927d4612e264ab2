#include <lcd.h>
#include "draw.h"



void lcd_draw_cricle(){
	uint16 x,y;
	 angle_circle = (j*0.01745331);       	 // angle expressed in radians - 1 degree = 0,01745331 radians
	   x = (x_center + (radius*cos(angle_circle)));
	   y= (y_center + (radius*sin(angle_circle)));
	   lcd_putpixel(x,y, 0xf);
}

